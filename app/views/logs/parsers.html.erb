<div>

<div class="form-group" id="log-patterns">
  <label for="sel1">Patterns:</label>
  <select class="form-control" id="sel1">
    <option>Grok</option>
  </select>
</div>

<div class="table">
  <table class="table table-striped">
    <thead>
      <tr>
        <th>Name</th>
        <th>Expression</th>
      </tr>
    </thead>
    <tbody>
			<% @patterns.each do |name, expression| %>
			  <tr>
			    <td id="name"><%= name %></td>
			    <td id="expression"><%= expression %></td>
			  </tr>
			<% end %>
		</tbody>
	</table>
</div>

</div>